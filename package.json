{
    "name": "bule-syntax-highlighting",
    "displayName": "Bule Syntax Highlighting",
    "description": "Syntax Highlighter for Bule",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.52.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "bule",
            "aliases": ["Bule", "bule"],
            "extensions": [".bul"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "bule",
            "scopeName": "source.bul",
            "path": "./syntaxes/bule.tmLanguage.json"
        }]
    }
}